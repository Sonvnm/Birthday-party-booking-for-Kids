@page
@model BirthdayPartyBookingForKids_Client.Pages.RegisterModel
@{
    ViewData["Title"] = "Register";
}
<h1>Register</h1>

<form method="post">
    <div class="form-group">
        <label>UserId:</label>
        <input type="text" id="UserViewModel.UserId" name="UserViewModel.UserId" class="form-control" required />
    </div>
    <div class="form-group">
        <label>Username:</label>
        <input type="text" id="UserViewModel.UserName" name="UserViewModel.UserName" class="form-control" required />
    </div>
    <div class="form-group">
        <label>Email:</label>
        <input type="text" id="UserViewModel.Email" name="UserViewModel.Email" class="form-control" required />
    </div>
    <div class="form-group">
        <label>Birth Day:</label>
        <input type="text" id="UserViewModel.BirthDay" name="UserViewModel.BirthDay" class="form-control" required />
    </div>
    <div class="form-group">
        <label>Password:</label>
        <input type="password" id="UserViewModel.Password" name="UserViewModel.Password" class="form-control" required />
    </div>
    <div class="form-group">
        <label>Phone:</label>
        <input type="text" id="UserViewModel.Phone" name="UserViewModel.Phone" class="form-control" required />
    </div>
    <div class="form-group">
        <label>RoleId:</label>
        <input type="text" id="UserViewModel.RoleId" name="UserViewModel.RoleId" class="form-control" required />
    </div>
    <button type="submit" class="btn btn-primary">Register</button>
</form>


